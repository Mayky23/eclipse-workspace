package OrganizadorTareas;

import javax.swing.*;

public class OrganizadorTareasApp {
    public static void main(String[] args) {
        SwingUtilities.invokeLater(new Runnable() {
            @Override
            public void run() {
                JFrame frame = new OrganizadorTareasFrame();
                frame.setVisible(true);
            }
        });
    }
}
